<script type="text/javascript">
$(function(){
	$("#btnlogin").click(function(){
		var username = $("#username").val();
		if(!username){
			alert("Do that again");
		}

		$.ajax({
			url:"http://127.0.0.1:8080/FrontDesk/api/profile",
			type:"post",
			data:{'username' : username},
			success : function(result){
				if(result){
					alert(result);
				}
			}
		});
	});
});

</script>


</head>


<body id="wrapper" screen_capture_injected="true" class="bs-docs-home">
